<form method="post">
  <h2>General Info</h2>
  Names
  <input type="text" name="names[]" required>
  <input type="text" name="names[]" >
  <br>
  Add Multiple Photos
  <input type="file" multiple>
  <br>
  {{> sex}}
  <br>
  {{> type}}
  <br>
  {{> status}}
  <br>
  Particular Signs
  <br>
  <textarea name="particularSigns"></textarea>
  <br>

  <h2>Rescue Info</h2>
  Rescued
  {{> age}}
  <br>
  Rescue Date
  <input type="date" name="rescued[date]" required>
  <br>
  Rescue Address
  <input type="text" name="rescued[address]" required>
  <br>
  Rescuers
  <input type="text" name="rescued[by]" required>
  <br>
  {{> organization}}
  <br>
  Notes
  <br>
  <textarea name="rescued[notes]"></textarea>
  <br>

  <br>
  <br>
  <input type="submit" />
</form>